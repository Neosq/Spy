if _G.SimpleSpyExecuted then
    if type(_G.SimpleSpyShutdown) == "function" then
        pcall(_G.SimpleSpyShutdown)
    end
    
    if SimpleSpy2 and SimpleSpy2.Parent then
        SimpleSpy2:Destroy()
    end
    if mobileToggle and mobileToggle.Parent then
        mobileToggle:Destroy()
    end
end

_G.SimpleSpyExecuted = true

_G.SimpleSpyShutdown = function()
    if SimpleSpy2 then SimpleSpy2:Destroy() end
    if mobileToggle then mobileToggle:Destroy() end
    _G.SimpleSpyExecuted = false
end

